export HOMEBREW_NO_ANALYTICS=1

if [ -s $HOME/Applications/Emacs.app/Contents/MacOS/Emacs ]
then
  export EDITOR="$HOME/Applications/Emacs.app/Contents/MacOS/Emacs"
fi

typeset -U path PATH

[ -s $HOME/.homebrew/bin/brew ] && eval "$($HOME/.homebrew/bin/brew shellenv)"

path=($HOME/.emacs.d/bin $HOME/.local/bin $path)

export VOLTA_HOME="$HOME/.volta"
path=($VOLTA_HOME/bin $path)

export PYENV_ROOT="$HOME/.pyenv"
path=($PYENV_ROOT/bin $path)
if type pyenv &>/dev/null
then
  eval "$(pyenv init -)"
fi

[ -s $HOME/.cargo/env ] && \. $HOME/.cargo/env

export PATH
export ZSHENV_PATH=$PATH
export ZSHENV_MANPATH=$MANPATH
